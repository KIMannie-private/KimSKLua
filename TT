        else
            OutputLogMessage("ON-NoRecoil-AS_VAL\n")
            if not IsKeyLockOn("scrolllock") then
                PressAndReleaseKey("scrolllock")
            end
        end
    elseif (event == "MOUSE_BUTTON_PRESSED" and arg == MACRO_OFF) or
        (event == "G_PRESSED" and arg == MACRO_OFF_Keyboard) then
        copy = not copy
        weapon = arg
        if (copy == false) then
            OutputLogMessage("OFF-Macro-MACRO_OFF\n")
            if IsKeyLockOn("scrolllock") then
                PressAndReleaseKey("scrolllock")
            end
        else
            OutputLogMessage("ON-NoRecoil-MACRO_OFF\n")
            if not IsKeyLockOn("scrolllock") then
                PressAndReleaseKey("scrolllock")
            end
        end
    end
    local remainder_fractional_x, remainder_fractional_y = 0, 0
    local function MoveMouseRelativeFractional(x, y)
        x = remainder_fractional_x + x
        y = remainder_fractional_y + y
        local x_int = math.floor(x + 0.5)
        local y_int = math.floor(y + 0.5)
        remainder_fractional_x = x - x_int
        remainder_fractional_y = y - y_int
        MoveMouseRelative(x_int, y_int)
    end
    if weapon == MACRO_OFF or weapon == MACRO_OFF_Keyboard then
        if copy then end
    end
