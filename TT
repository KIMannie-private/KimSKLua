-- ========================================= MACROON =========================================---
EnablePrimaryMouseButtonEvents(true);
local copy = false
local paste = 0
local CSMacro = 1
function OnEvent(event, arg)
    if (event == "MOUSE_BUTTON_PRESSED" and arg == SCAR_H) or
        (event == "G_PRESSED" and arg == SCAR_H_Keyboard) then
        copy = not copy
        weapon = arg
        if (copy == false) then
            OutputLogMessage("OFF-Macro-SCAR_H\n")
            if IsKeyLockOn("scrolllock") then
                PressAndReleaseKey("scrolllock")
            end
        else
            OutputLogMessage("ON-NoRecoil-SCAR_H\n")
            if not IsKeyLockOn("scrolllock") then
                PressAndReleaseKey("scrolllock")
            end
        end
    elseif (event == "MOUSE_BUTTON_PRESSED" and arg == Rapid_Fire) or
        (event == "G_PRESSED" and arg == Rapid_Fire_Keyboard) then
        copy = not copy
        weapon = arg
        if (copy == false) then
            OutputLogMessage("OFF-Macro-Rapid_Fire\n")
            if IsKeyLockOn("scrolllock") then
                PressAndReleaseKey("scrolllock")
            end
        else
            OutputLogMessage("ON-NoRecoil-Rapid_Fire\n")
            if not IsKeyLockOn("scrolllock") then
                PressAndReleaseKey("scrolllock")
            end
        end
