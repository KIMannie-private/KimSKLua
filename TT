    if weapon == VITYAZ or weapon == VITYAZ_Keyboard then
        if copy then
            local CSMacro = VITYAZ_Optimization
            if IsMouseButtonPressed(click) then
                repeat
                    csm(18)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(-1 * CSMacro *
                                                    VITYAZ_Optimization, 6 *
                                                    CSMacro *
                                                    VITYAZ_Optimization)
                    csm(18)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(-1 * CSMacro *
                                                    VITYAZ_Optimization, 6 *
                                                    CSMacro *
