    if weapon == AS_VAL or weapon == AS_VAL_Keyboard then
        if copy then
            local CSMacro = AS_VAL_Optimization
            if IsMouseButtonPressed(click) then
                repeat
                    csm(18)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(1 * CSMacro *
                                                    AS_VAL_Optimization, 5 *
                                                    CSMacro *
