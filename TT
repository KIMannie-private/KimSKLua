    if weapon == PKM or weapon == PKM_Keyboard then
        if copy then
            local CSMacro = PKM_Optimization
            if IsMouseButtonPressed(click) then
                repeat
                    csm(85)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(0 * CSMacro * PKM_Optimization,
                                                16 * CSMacro * PKM_Optimization)
                    csm(18)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(0 * CSMacro * PKM_Optimization,
                                                15 * CSMacro * PKM_Optimization)
                    csm(18)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(0 * CSMacro * PKM_Optimization,
                                                13 * CSMacro * PKM_Optimization)
                    csm(18)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(1 * CSMacro * PKM_Optimization,
                                                10 * CSMacro * PKM_Optimization)
                    csm(18)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(0 * CSMacro * PKM_Optimization,
                                                10 * CSMacro * PKM_Optimization)
                    csm(18)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(1 * CSMacro * PKM_Optimization,
                                                10 * CSMacro * PKM_Optimization)
                    csm(18)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(1 * CSMacro * PKM_Optimization,
                                                10 * CSMacro * PKM_Optimization)
                    csm(18)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(1 * CSMacro * PKM_Optimization,
                                                9 * CSMacro * PKM_Optimization)
                    csm(18)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(1 * CSMacro * PKM_Optimization,
                                                9 * CSMacro * PKM_Optimization)
                    csm(18)
                    if not IsMouseButtonPressed(1) then break end
                    MoveMouseRelativeFractional(1 * CSMacro * PKM_Optimization,
                                                8 * CSMacro * PKM_Optimization)
